ticket_id, description, created_at in support_tickets:\\n    start_index = description.find(\\\"Project ID:\\\")\\n    end_index = description.find(\\\"\\\\n\\\", start_index)\\n    project_id = description[start_index + 11:end_index].strip()\\n    plan_id = project_plans.get(project_id)\\n    plan_name = plan_names.get(plan_id, \\\"unknown\\\")\\n    classified_category = classify_ticket(description)\\n    print(f\\\"Ticket {ticket_id}: project_id={project_id}, plan_name={plan_name}, created_at={created_at}, category={classified_category}\\\")\\n    extracted_tickets.append((ticket_id, project_id, plan_name, created_at, classified_category))\\n\\n# Define a function to sort based on priority criteria\\ndef sort_keys(ticket):\\n    plan_name, category, created_at = ticket[2], ticket[4], ticket[3]\\n    priority = 0\\n\\n    # advanced plan with 'downtime' or 'reliability'\\n    if plan_name == \\\"advanced\\\" and category in [\\\"downtime\\\", \\\"reliability\\\"]:\\n        priority = 1\\n    # base plan with 'downtime' or 'reliability'\\n    elif plan_name == \\\"base\\\" and category in [\\\"downtime\\\", \\\"reliability\\\"]:\\n        priority = 2\\n    # Prioritize based on categories\\n    elif category == \\\"downtime\\\":\\n        priority = 3\\n    elif category == \\\"reliability\\\":\\n        priority = 4\\n    elif category == \\\"performance\\\":\\n        priority = 5\\n    elif category == \\\"bug\\\":\\n        priority = 6\\n    elif category == \\\"feature request\\\":\\n        priority = 7\\n    else:\\n        priority = 8\\n\\n    return (priority, created_at)\\n\\n# Sort tickets using the defined keys\\nsorted_tickets = sorted(extracted_tickets, key=sort_keys)[:5]\\n\\nprint(sorted_tickets)\"}",
