ority == 1 and (category == \\\"downtime\\\" or category == \\\"reliability\\\"):\\n        return 0\\n    if priority == 0 and category == \\\"downtime\\\":\\n        return 1\\n    return 2\\n\\n\\nticket_details = []\\nfor ticket in support_tickets_data:\\n    project_id = re.search(r'[0-9a-fA-F-]{36}', ticket['description']).group()\\n    category = decide_category(ticket['description'])\\n    ticket_details.append((ticket['id'], project_id, ticket['created_at'], category))\\n\\ndef compare_tickets(ticket1, ticket2):\\n    priority1 = get_ticket_priority(ticket1)\\n    priority2 = get_ticket_priority(ticket2)\\n    if priority1 < priority2:\\n        return -1\\n    elif priority1 > priority2:\\n        return 1\\n    else:\\n        category_priority_1 = plans_data.index(ticket1[3])\\n        category_priority_2 = plans_data.index(ticket2[3])\\n        if category_priority_1 < category_priority_2:\\n            return -1\\n        elif category_priority_1 > category_priority_2:\\n            return 1\\n        else:\\n            return (ticket1[2] > ticket2[2]) - (ticket1[2] < ticket2[2])\\n\\n\\nsorted_tickets = sorted(ticket_details, key=cmp_to_key(compare_tickets))\\n\\n# Return top 5 ticket ids\\nprint([ticket[0] for ticket in sorted_tickets[:5]])\",\"dataValues\":\"\"}",
