y priority\\n    category_order = {\\n        \\'downtime\\': 0,\\n        \\'reliability\\': 1,\\n        \\'performance\\': 2,\\n        \\'bug\\': 3,\\n        \\'feature request\\': 4,\\n        \\'others\\': 5\\n    }\\n    \\n    if category_order[a[\\'category\\']] != category_order[b[\\'category\\']]:\\n        return category_order[a[\\'category\\']] - category_order[b[\\'category\\']]\\n    \\n    # Rule 3: Creation time (older first)\\n    a_time = datetime.fromisoformat(a[\\'created_at\\'].replace(\\'Z\\', \\'+00:00\\'))\\n    b_time = datetime.fromisoformat(b[\\'created_at\\'].replace(\\'Z\\', \\'+00:00\\'))\\n    return -1 if a_time < b_time else 1 if a_time > b_time else 0\\n\\n# Prepare tickets data\\ntickets_data = [\\n    {\"id\": 143, \"subject\": \"URGENT: Cannot Access Project\", \"description\": \"I am unable to access my project on Hasura Cloud. Project ID: 2a8d4b07-c391-450f-8153-f6521f8a20a0\", \"created_at\": \"2025-01-03T20:32:35.345840+00:00\"},\\n    {\"id\": 166, \"subject\": \"Feature Request: Support for Custom GraphQL Directives\", \"description\": \"Feature request for custom directives. Project ID: 2a8d4b07-c391-450f-8153-f6521f8a20a0\", \"created_at\": \"2024-12-21T14:32:35.345840+00:00\"},\\n    {\"id\": 209, \"subject\": \"Urgent: Production Environment Down\", \"description\": \"Production environment down. Project ID: 76de9616-3d7b-49d7-b909-e154dda91489\", \"created_at\": \"2024-12-05T07:00:00+00:00\"},\\n    {\"id\": 169, \"subject\": \"Schema Registry causing high resource usage\", \"description\": \"High resource usage in production. Project ID: 386d8856-9020-47eb-a21a-2ebe6b81428e\", \"created_at\": \"2024-11-12T05:00:00+00:00\"},\\n    {\"id\": 251, \"subject\": \"Performance degradation in operations\", \"description\": \"Performance issues. Project ID: d5dc02fc-707a-4084-854a-79f5866d4dd3\", \"created_at\": \"2024-10-25T04:00:00+00:00\"},\\n    {\"id\": 139, \"subject\": \"Cannot access project - urgent\", \"descriptio