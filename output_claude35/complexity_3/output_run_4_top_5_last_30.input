      "TextBlock(citations=None, text=\"Now, let's get the plan information for all these projects:\", type='text')",
      "ToolUseBlock(id='toolu_017RvJxHCCDsopnsD9gBBbDR', input={'sql': 'WITH latest_plan_changes AS (\\n    SELECT \\n        project_id,\\n        plan_id,\\n        created_at,\\n        ROW_NUMBER() OVER (PARTITION BY project_id ORDER BY created_at DESC) as rn\\n    FROM project_plan_changelogs\\n)\\nSELECT \\n    lpc.project_id,\\n    p.name as plan_name\\nFROM latest_plan_changes lpc\\nJOIN plans p ON p.id = lpc.plan_id\\nWHERE lpc.rn = 1;'}, name='query_control_plane_data', type='tool_use')"
    ]
  },
