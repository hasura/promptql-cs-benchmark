          "id": "toolu_01Xprau72LnWVohhj8swwaSH",
          "input": {
            "pythonCode": "import json\nfrom datetime import datetime\nfrom functools import cmp_to_key\n\ndef extract_project_id(description):\n    # Look for Project ID pattern\n    lines = description.split('\\n')\n    for line in lines:\n        if 'Project ID:' in line:\n            parts = line.split('Project ID:')\n            if len(parts) > 1:\n                return parts[1].strip()\n    return None\n\ndef classify_ticket(subject, description):\n    text = (subject + \" \" + description).lower()\n    \n    # Downtime indicators\n    if any(word in text for word in ['cannot access', 'unreachable', 'down', 'outage', '500 error', 'service disruption']):\n        if 'widespread' in text or 'production' in text:\n            return 'downtime'\n    \n    # Reliability indicators\n    if any(word in text for word in ['intermittent', 'random', 'unstable', 'breaking']):\n        if 'production' in text or 'reliability' in text:\n            return 'reliability'\n    \n    # Performance indicators\n    if any(word in text for word in ['slow', 'performance', 'latency', 'resource exhaustion']):\n        return 'performance'\n    \n    # Bug indicators\n    if any(word in text for word in ['bug', 'incorrect', 'wrong result']):\n        return 'bug'\n    \n    # Feature request indicators\n    if 'feature request' in text or 'enhancement' in text:\n        return 'feature request'\n    \n    return 'others'\n\ndef compare_tickets(a, b):\n    # Rule 1: Advanced plan with downtime/reliability\n    a_priority = 2 if a['plan'] == 'advanced' and a['category'] in ['downtime', 'reliability'] else 0\n    b_priority = 2 if b['plan'] == 'advanced' and b['category'] in ['downtime', 'reliability'] else 0\n    \n    if a_priority != b_priority:\n        return b_priority - a_priority\n    \n    # Rule 2: Category priority\n    category_priority = {\n        'downtime': 6,\n        'reliability': 5,\n        'performance': 4,\n        'bug': 3,\n        'feature request': 2,\n        'others': 1\n    }\n    \n    if category_priority[a['category']] != category_priority[b['category']]:\n        return category_priority[a['category']] - category_priority[b['category']]\n    \n    # Rule 3: Creation time (older first)\n    return (datetime.fromisoformat(a['created_at']) - datetime.fromisoformat(b['created_at'])).total_seconds()\n\n# Process tickets\ntickets = []\nproject_plans = {item['project_id']: item['plan_name'] for item in json.loads(input_data[1])}\n\nfor ticket in json.loads(input_data[0]):\n    project_id = extract_project_id(ticket['description'])\n    category = classify_ticket(ticket['subject'], ticket['description'])\n    \n    tickets.append({\n        'id': ticket['id'],\n        'project_id': project_id,\n        'category': category,\n        'created_at': ticket['created_at'],\n        'plan': project_plans.get(project_id, 'free')\n    })\n\n# Sort tickets\nsorted_tickets = sorted(tickets, key=cmp_to_key(compare_tickets), reverse=True)\n\n# Output top 5 ticket IDs\nprint(\"<tickets>\")\nfor ticket in sorted_tickets[:5]:\n    print(ticket['id'])\nprint(\"</tickets>\")"
